import java.util.*;
class table
{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println();
  
	System.out.println("                                          This Is java Multiplication Table ");	
	
  System.out.println();
	
  System.out.print("Enter Any Number:>=}");
	 int num = sc.nextInt();
        for(int i = 1; i <= 10; i++)
        {
            System.out.printf("%d * %d = %d \n", num, i, num * i);
        }
    }
}
